    "babel-preset-next": "^1.2.0",
    "cookies": "^0.8.0",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "graphql": "^14.5.8",
